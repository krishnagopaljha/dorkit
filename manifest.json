{
  "manifest_version": 3,
  "name": "Dork It",
  "description": "A tool for advanced Google Dorking with customizable search parameters.",
  "version": "1.0",
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    }
  },
  "permissions": [
    "tabs",
    "activeTab"
  ]
}
